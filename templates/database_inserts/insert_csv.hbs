{{#each tables}}
INSERT INTO {{this.table_name}} (
    {{#each this.columns}} 
        {{this.name}}
        {{#unless @last}},{{/unless}} 
    {{/each}}
    ) VALUES
    {{#each this.document.rows}}
    (
        {{#each this}}
            '{{this}}'
            {{#unless @last}},{{/unless}} 
        {{/each}}
    )
    {{#unless @last}},{{/unless}} 
    {{/each}}
);
{{/each}}